{
    "private": true,
    "name": "flutter_socket_io_backend",
    "version": "1.0.0",
    "description": "Socket.IO backend of Flutter App",
    "main": "app.js",
    "scripts": {
        "dev": "nodemon index.js",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
