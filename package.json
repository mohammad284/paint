        "postcss": "^8.1.14",
        "pusher-js": "^7.1.1-beta"
    },
    "dependencies": {
        "express": "^4.18.1",
        "http": "^0.0.1-security",
        "mysql": "^2.18.1",
        "nodemon": "^2.0.16",
        "socket.io": "^4.4.1"
    }
}
